{
    "version": "2.4",
    "name": "Intune",
    "help": "https://intunewiki.com/wiki/Intune_Azure_Portal",
    "changelog": [
      {
        "version": "2.4",
        "date": "April 14, 2021",
        "notes": "FIXED: Canary sideloading in EDU."
      },
      {
        "version": "2.3",
        "date": "December 30, 2020",
        "notes": "FIXED: Removed MTM from the local extensions list of the Endpoint config.\nFIXED: Mock data toggle in EDU."
      },
      {
        "version": "2.1",
        "date": "December 30, 2020",
        "notes": "FIXED: Default to showing the first config if none of the config URLs match.\nADDED: A changelog."
      }
    ],
    "configs": [
      {
        "name": "Intune - Endpoint",
        "environments": [
          {
              "label": "DF",
              "host": "df-devicemanagement.onecloud.azure-test.net"
          },
          {
              "label": "Selfhost",
              "host": "endpoint.microsoft.com",
              "params": {
                  "feature.canmodifystamps": "true",
                  "feature.canmodifyextensions": "true",
                  "feature.Microsoft_Intune": "sh",
                  "Microsoft_Intune": "sh",
                  "Microsoft_Intune_Apps": "selfhost",
                  "Microsoft_Intune_Devices": "selfhost",
                  "Microsoft_Intune_DeviceSettings": "selfhost",
                  "Microsoft_Intune_Enrollment": "selfhost",
                  "Microsoft_Intune_Workflows": "selfhost"
              }
          },
          {
              "label": "Canary",
              "host": "endpoint.microsoft.com",
              "params": {
                  "feature.canmodifystamps": "true",
                  "feature.canmodifyextensions": "true",
                  "feature.Microsoft_Intune": "ctip",
                  "Microsoft_Intune": "ctip",
                  "Microsoft_Intune_Apps": "canary",
                  "Microsoft_Intune_Devices": "canary",
                  "Microsoft_Intune_DeviceSettings": "canary",
                  "Microsoft_Intune_Enrollment": "canary",
                  "Microsoft_Intune_Workflows": "canary"
              }
          },
          {
              "label": "TestProd",
              "host": "endpoint.microsoft.com",
              "params": {
                  "feature.canmodifystamps": "true",
                  "feature.canmodifyextensions": "true",
                  "feature.Microsoft_Intune": "testprod",
                  "Microsoft_Intune": "testprod",
                  "Microsoft_Intune_Apps": "testprod",
                  "Microsoft_Intune_Devices": "testprod",
                  "Microsoft_Intune_DeviceSettings": "testprod",
                  "Microsoft_Intune_Enrollment": "testprod",
                  "Microsoft_Intune_Workflows": "testprod"
              }
          },
          {
              "label": "Prod",
              "host": "endpoint.microsoft.com",
              "params": {
                  "Microsoft_Intune_Apps": "prod",
                  "Microsoft_Intune_Devices": "prod",
                  "Microsoft_Intune_DeviceSettings": "prod",
                  "Microsoft_Intune_Workflows": "prod"
              }
          },
          {
              "label": "Fairfax",
              "host": "endpoint.microsoft.us"
          },
          {
              "label": "MC",
              "host": "endpoint.microsoftonline.cn"
          }
      ],
      "localExtensions": [
          "Microsoft_Intune",
          "Microsoft_Intune_Apps",
          "Microsoft_Intune_Devices",
          "Microsoft_Intune_DeviceSettings",
          "Microsoft_Intune_Enrollment",
          "Microsoft_Intune_Workflows"
      ],
      "featureGroups": [
          {
              "label": "Global features",
              "features": [
                  {
                      "label": "Client optimizations",
                      "name": "clientOptimizations",
                      "options": [
                          "true",
                          "false"
                      ]
                  },
                  {
                      "label": "Intune service environment (defaults to current env)",
                      "name": "feature.intune_env",
                      "options": [
                          "df",
                          "sh",
                          "canary",
                          "prod",
                          "fairfax",
                          "mooncake"
                      ]
                  },
                  {
                      "label": "Mock data",
                      "name": "feature.intune_mockgraph",
                      "options": [
                          "true",
                          "false"
                      ]
                  }
              ]
          }
      ],
      "dynamicFeatureGroups": [
          {
              "label": "Enrollment",
              "prefix": "microsoft_intune_enrollment_",
              "source": {
                  "DF": "https://hosting.onecloud.azure-test.net/intuneenrollment/Content/Scripts/Shared/Features.js",
                  "Selfhost": "https://intuneenrollmentsh.hosting.portal.azure.net/intuneenrollmentsh/Content/Scripts/Shared/Features.js",
                  "Canary": "https://intuneenrollmentctip.hosting.portal.azure.net/intuneenrollmentctip/Content/Scripts/Shared/Features.js",
                  "TestProd": "https://intuneenrollment.hosting.portal.azure.net/intuneenrollment/Content/Scripts/Shared/Features.js",
                  "Prod": "https://intuneenrollment.hosting.portal.azure.net/intuneenrollment/Content/Scripts/Shared/Features.js",
                  "Fairfax": "https://fxp.enrollment.mdm.ext.azure.us/Content/Scripts/Shared/Features.js"
              }
          },
          {
              "label": "Devices",
              "prefix": "microsoft_intune_devices_",
              "source": {
                  "DF": "https://hosting.onecloud.azure-test.net/intunedevices/Content/Scripts/Shared/Features.js",
                  "Selfhost": "https://intunedevicessh.hosting.portal.azure.net/intunedevicessh/Content/Scripts/Shared/Features.js",
                  "Canary": "https://intunedevicesctip.hosting.portal.azure.net/intunedevicesctip/Content/Scripts/Shared/Features.js",
                  "TestProd": "https://intunedevices.hosting.portal.azure.net/intunedevices/Content/Scripts/Shared/Features.js",
                  "Prod": "https://intunedevices.hosting.portal.azure.net/intunedevices/Content/Scripts/Shared/Features.js",
                  "Fairfax": "https://fxp.devices.mdm.ext.azure.us/Content/Scripts/Shared/Features.js"
              }
          }
      ]
    },
    {
      "name":"Intune - EDU",
      "environments": [
        {
            "label": "DF",
            "host": "df-intune-edu.onecloud.azure-test.net"
        },
        {
            "label": "Selfhost",
            "host": "intuneeducation.portal.azure.com",
            "params": {
                "feature.canmodifystamps": "true",
                "feature.canmodifyextensions": "true",
                "Microsoft_Intune_Edu": "selfhost"
            }
        },
        {
            "label": "Canary",
            "host": "intuneeducation.portal.azure.com",
            "params": {
                "feature.canmodifystamps": "true",
                "feature.canmodifyextensions": "true",
                "Microsoft_Intune_Edu": "canary"
            }
        },
        {
            "label": "TestProd",
            "host": "intuneeducation.portal.azure.com",
            "params": {
                "feature.canmodifystamps": "true",
                "feature.canmodifyextensions": "true",
                "Microsoft_Intune_Edu": "testprod"
            }
        },
        {
            "label": "Prod",
            "host": "intuneeducation.portal.azure.com"
        }
    ],
    "localExtensions": [
        "Microsoft_Intune_Edu"
    ],
    "featureGroups": [
        {
            "label": "Global features",
            "features": [
                {
                    "label": "Client optimizations",
                    "name": "clientOptimizations",
                    "options": [
                        "true",
                        "false"
                    ]
                },
                {
                    "label": "Mock data",
                    "name": "feature.configuration",
                    "options": [
                        "UseMockData"
                    ]
                }
            ]
        }
      ]
    },
    {
      "name":"Intune - MTM",
      "environments": [
        {
            "label": "Selfhost",
            "host": "rc-m365multitenantmanagement.portal.azure.com"
        },
        {
            "label": "Prod",
            "host": "lighthouse.microsoft.com"
        }
    ],
    "localExtensions": [
        "Microsoft_Intune_MTM"
    ],
    "featureGroups": [
        {
            "label": "Global features",
            "features": [
                {
                    "label": "Client optimizations",
                    "name": "clientOptimizations",
                    "options": [
                        "true",
                        "false"
                    ]
                },
                {
                  "label": "React reload",
                  "name": "feature.reactreload",
                  "options": [
                      "true",
                      "false"
                  ]
              }
            ]
        }
      ]
    }
  ]
}
